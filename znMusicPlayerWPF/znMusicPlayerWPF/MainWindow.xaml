                        </ListBox>
                        <MyC:znButton x:Name="ShowBox_Normal_Button" Content="确定" Margin="0,0,35,15" Background="{DynamicResource ButtonPAMP}" BorderBrush="{x:Null}" FontSize="15" Click="ShowBox_ButtonClick" Foreground="{DynamicResource ATextColor_InColor}" HorizontalAlignment="Right" Width="76" Height="33" VerticalAlignment="Bottom" EnterColor="{DynamicResource ButtonPAMM}"/>
                    </Grid>
                    <Grid x:Name="ShowBox_Loading" Visibility="Visible">
                        <StackPanel>
                            <TextBlock x:Name="ShowBox_Loading_Title" Margin="24,14,24,0" VerticalAlignment="Top" Foreground="{DynamicResource ATextColor}" FontSize="24" TextTrimming="CharacterEllipsis" TextAlignment="Left" FontWeight="Normal" HorizontalAlignment="Left" Text="标题"/>
                            <MyC:SimpleLoading x:Name="ShowBox_Loading_SimpleLoading" StrokeThickness="1.2" Pause="True" Background="Transparent" Margin="80,6,80,0" Height="50" VerticalAlignment="Top" Foreground="{DynamicResource ButtonPAMP}" Width="50" HorizontalAlignment="Center"/>
                            <TextBlock x:Name="ShowBox_Loading_Text" Margin="24,6,24,0" TextAlignment="Center" Foreground="{DynamicResource ALittleTextColor}" FontSize="18" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Disabled" VerticalAlignment="Top" Text="内容" HorizontalAlignment="Center"/>
                            <MyC:znButton x:Name="ShowBox_Loading_Button" Content="取消" Margin="0,6,0,14" Background="{DynamicResource ButtonPAMP}" BorderBrush="{x:Null}" FontSize="15" Click="ShowBox_ButtonClick" Foreground="{DynamicResource ATextColor_InColor}" HorizontalAlignment="Center" Width="100" Height="34" VerticalAlignment="Top" EnterColor="{DynamicResource ButtonPAMM}"/>
                        </StackPanel>
                    </Grid>
                    <Grid x:Name="ShowBox_UserDesktopChanger" Width="800" Visibility="Collapsed">
                        <TextBlock x:Name="ShowBox_UserDesktopChanger_Title" Margin="25,15,25,0" Text="标题" VerticalAlignment="Top" Foreground="{DynamicResource ATextColor}" FontSize="24" TextTrimming="CharacterEllipsis" TextAlignment="Left" FontWeight="Normal" HorizontalAlignment="Left"/>
                        <TextBlock x:Name="ShowBox_UserDesktopChanger_Text" Margin="25,55,35,330" Foreground="{DynamicResource ALittleTextColor}" FontSize="18" TextTrimming="CharacterEllipsis" TextWrapping="Wrap" ScrollViewer.HorizontalScrollBarVisibility="Visible" ScrollViewer.VerticalScrollBarVisibility="Disabled"><Run Text="内容"/><Run Text="测试"/><LineBreak/><Run/></TextBlock>
                        <MyC:znButton x:Name="ShowBox_UserDesktopChanger_Button_Save" Content="保存" Margin="0,0,25,15" Background="#7F000000" BorderBrush="{x:Null}" FontSize="15" Click="ShowBox_ButtonClick" Foreground="{DynamicResource ATextColor_InColor}" HorizontalAlignment="Right" Width="76" Height="38" VerticalAlignment="Bottom"/>
                        <Image x:Name="ShowBox_UserDesktopChanger_Image" HorizontalAlignment="Left" Height="250" Margin="25,83,0,0" VerticalAlignment="Top" Width="250"/>
                    </Grid>
                    <Grid x:Name="ShowBox_MusicDatas" Visibility="Collapsed">
                        <TextBlock x:Name="ShowBox_MusicDatas_Title" Margin="25,15,25,0" Text="标题" VerticalAlignment="Top" Foreground="{DynamicResource ATextColor}" FontSize="24" TextTrimming="CharacterEllipsis" TextAlignment="Left" FontWeight="Normal" HorizontalAlignment="Left"/>
                        <StackPanel Margin="25,65,25,55" VerticalAlignment="Top">
                            <StackPanel Orientation="Horizontal">
                                <MyC:znButton x:Name="ShowBox_MusicDatas_TitleText_Button" Content="标题" HorizontalAlignment="Left" Margin="0" VerticalAlignment="Top" Height="23" Background="{DynamicResource ButtonPAMP}" BorderBrush="{x:Null}" Foreground="{DynamicResource ATextColor_InColor}" Click="ShowBox_MusicDatas_ButtonsClick" Width="60"/>
                                <TextBlock x:Name="ShowBox_MusicDatas_TitleText" Text="标题" Margin="5,0,0,0" Foreground="{DynamicResource ALittleTextColor}" FontSize="18" TextTrimming="CharacterEllipsis" TextWrapping="NoWrap" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Disabled" VerticalAlignment="Top" HorizontalAlignment="Left"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <MyC:znButton x:Name="ShowBox_MusicDatas_ArtistText_Button" Content="歌手" HorizontalAlignment="Left" Margin="0" VerticalAlignment="Top" Height="23" Background="{DynamicResource ButtonPAMP}" BorderBrush="{x:Null}" Foreground="{DynamicResource ATextColor_InColor}" Click="ShowBox_MusicDatas_ButtonsClick" Width="60"/>
                                <TextBlock x:Name="ShowBox_MusicDatas_ArtistText" Text="歌手" Margin="5,0,0,0" Foreground="{DynamicResource ALittleTextColor}" FontSize="18" TextTrimming="CharacterEllipsis" TextWrapping="NoWrap" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Disabled" VerticalAlignment="Top" HorizontalAlignment="Left"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <MyC:znButton x:Name="ShowBox_MusicDatas_AlbumText_Button" Content="专辑" HorizontalAlignment="Left" Margin="0" VerticalAlignment="Top" Height="23" Background="{DynamicResource ButtonPAMP}" BorderBrush="{x:Null}" Foreground="{DynamicResource ATextColor_InColor}" Click="ShowBox_MusicDatas_ButtonsClick" Width="60"/>
                                <TextBlock x:Name="ShowBox_MusicDatas_AlbumText" Text="专辑" Margin="5,0,0,0" Foreground="{DynamicResource ALittleTextColor}" FontSize="18" TextTrimming="CharacterEllipsis" TextWrapping="NoWrap" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Disabled" VerticalAlignment="Top" HorizontalAlignment="Left"/>
                            </StackPanel>
                            <StackPanel x:Name="ShowBox_MusicDatas_IDText_s" Orientation="Horizontal">
                                <MyC:znButton x:Name="ShowBox_MusicDatas_IDText_Button" Content="歌曲ID" HorizontalAlignment="Left" Margin="0" VerticalAlignment="Top" Height="23" Background="{DynamicResource ButtonPAMP}" BorderBrush="{x:Null}" Foreground="{DynamicResource ATextColor_InColor}" Click="ShowBox_MusicDatas_ButtonsClick" Width="60" AnimationColor="#64000000"/>
                                <TextBlock x:Name="ShowBox_MusicDatas_IDText" Text="ID" Margin="5,0,0,0" Foreground="{DynamicResource ALittleTextColor}" FontSize="18" TextTrimming="CharacterEllipsis" TextWrapping="NoWrap" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Disabled" VerticalAlignment="Top" HorizontalAlignment="Left"/>
                            </StackPanel>
