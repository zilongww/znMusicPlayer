        xmlns:pcontent="clr-namespace:znMusicPlayerWPF.MyC.PopupContent"
        xmlns:Pages="clr-namespace:znMusicPlayerWPF.Pages"
        xmlns:MyC="clr-namespace:znMusicPlayerWPF.MyC"
        xmlns:b="http://schemas.microsoft.com/xaml/behaviors" xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" xmlns:ui="http://schemas.modernwpf.com/2019" x:Name="MainActivity" x:Class="znMusicPlayerWPF.MainWindow"
        mc:Ignorable="d" DpiChanged="MainActivity_DpiChanged" StateChanged="MainActivity_StateChanged" BorderThickness="0"
        TextOptions.TextHintingMode="Fixed" Deactivated="MainActivity_Deactivated" Activated="MainActivity_Activated" WindowStyle="SingleBorderWindow"
        Title="MainWindow" Width="1145" MinWidth="520" MinHeight="114" Height="630" WindowStartupLocation="CenterScreen" Background="{x:Null}" Icon="Images/SugarAndSalt.jpg" KeyDown="MainActivity_KeyDown" KeyUp="MainActivity_KeyUp">
    
