                    <StackPanel Margin="0,2,0,2">
                        <Grid Height="46" Width="202" HorizontalAlignment="Left">
                            <Grid x:Name="MainBtnBack">
                                <Rectangle Fill="{DynamicResource ALineColor}" RadiusY="6" RadiusX="6" Opacity="0.3"/>
                                <Rectangle Fill="{DynamicResource ButtonPAMP}" Width="4" Height="24" VerticalAlignment="Center" HorizontalAlignment="Left" RadiusY="2" RadiusX="2" Margin="2,0,0,0" />
                            </Grid>
                            <MyC:znButton x:Name="MainBtn" ShowContent="True" VerticalAlignment="Stretch" BorderBrush="{x:Null}" Background="#00000000" HorizontalAlignment="Left" Width="203" Height="46" Click="MainBtn_Checked" AnimationColor="{DynamicResource ButtonBlurPAMM}">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Width="194">
                                    <materialDesign:PackIcon Kind="HomeOutline" Margin="8,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Width="26" Height="26" Foreground="{DynamicResource ATextColor}"/>
                                    <TextBlock Margin="18,0,0,0" TextWrapping="NoWrap" Text="主页" FontSize="16" FontWeight="Normal" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{DynamicResource ATextColor}"/>
                                </StackPanel>
                            </MyC:znButton>
                        </Grid>
                        <Grid Height="46" Width="202" HorizontalAlignment="Left" Margin="0,2,0,0">
                            <Grid x:Name="SearchBtnBack">
                                <Rectangle Fill="{DynamicResource ALineColor}" RadiusY="6" RadiusX="6" Opacity="0.3"/>
                                <Rectangle Fill="{DynamicResource ButtonPAMP}" Width="4" Height="24" VerticalAlignment="Center" HorizontalAlignment="Left" RadiusY="2" RadiusX="2" Margin="2,0,0,0" />
                            </Grid>
                            <MyC:znButton x:Name="SearchBtn" ShowContent="True" VerticalAlignment="Stretch" BorderBrush="{x:Null}" Background="#00000000" HorizontalAlignment="Left" Width="202" Height="46" Click="MainBtn_Checked">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Width="194">
                                    <materialDesign:PackIcon Kind="Search" Margin="8,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Width="26" Height="26" Foreground="{DynamicResource ATextColor}"/>
                                    <TextBlock Margin="18,0,0,0" TextWrapping="NoWrap" Text="搜索" FontSize="16" FontWeight="Normal" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{DynamicResource ATextColor}"/>
                                </StackPanel>
                            </MyC:znButton>
                        </Grid>
                        <Grid Height="46" Width="202" HorizontalAlignment="Left" Margin="0,2,0,0">
                            <Grid x:Name="DownloadBtnBack">
                                <Rectangle Fill="{DynamicResource ALineColor}" RadiusY="6" RadiusX="6" Opacity="0.3"/>
                                <Rectangle Fill="{DynamicResource ButtonPAMP}" Width="4" Height="24" VerticalAlignment="Center" HorizontalAlignment="Left" RadiusY="2" RadiusX="2" Margin="2,0,0,0" />
                            </Grid>
                            <MyC:znButton x:Name="DownloadBtn" ShowContent="True" VerticalAlignment="Stretch" BorderBrush="{x:Null}" Background="#00000000" HorizontalAlignment="Left" Width="202" Height="46" Click="MainBtn_Checked">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Width="194">
                                    <materialDesign:PackIcon Kind="DownloadOutline" Margin="8,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Width="26" Height="26" Foreground="{DynamicResource ATextColor}"/>
                                    <TextBlock Margin="18,0,0,0" TextWrapping="NoWrap" Text="下载列表" FontSize="16" FontWeight="Normal" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{DynamicResource ATextColor}"/>
                                </StackPanel>
                            </MyC:znButton>
                        </Grid>
                        <Grid Height="46" Width="202" HorizontalAlignment="Left" Margin="0,2,0,0">
                            <Grid x:Name="PlayListBtnBack">
                                <Rectangle Fill="{DynamicResource ALineColor}" RadiusY="6" RadiusX="6" Opacity="0.3"/>
                                <Rectangle Fill="{DynamicResource ButtonPAMP}" Width="4" Height="24" VerticalAlignment="Center" HorizontalAlignment="Left" RadiusY="2" RadiusX="2" Margin="2,0,0,0" />
                            </Grid>
                            <MyC:znButton x:Name="PlayListBtn" ShowContent="True" VerticalAlignment="Stretch" BorderBrush="{x:Null}" Background="#00000000" HorizontalAlignment="Left" Width="202" Height="46" Click="MainBtn_Checked">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Width="194">
                                    <materialDesign:PackIcon Kind="PlaylistMusicOutline" Margin="8,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Width="26" Height="26" Foreground="{DynamicResource ATextColor}"/>
                                    <TextBlock Margin="18,0,0,0" TextWrapping="NoWrap" Text="播放列表" FontSize="16" FontWeight="Normal" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{DynamicResource ATextColor}"/>
                                </StackPanel>
                            </MyC:znButton>
                        </Grid>
                        <Grid Height="46" Width="202" HorizontalAlignment="Left" Margin="0,2,0,0">
                            <Grid x:Name="SettingBtnBack">
                                <Rectangle Fill="{DynamicResource ALineColor}" RadiusY="6" RadiusX="6" Opacity="0.3"/>
                                <Rectangle Fill="{DynamicResource ButtonPAMP}" Width="4" Height="24" VerticalAlignment="Center" HorizontalAlignment="Left" RadiusY="2" RadiusX="2" Margin="2,0,0,0" />
                            </Grid>
                            <MyC:znButton x:Name="SettingBtn" ShowContent="True" VerticalAlignment="Stretch" BorderBrush="{x:Null}" Background="#00000000" HorizontalAlignment="Left" Width="202" Height="46" Click="MainBtn_Checked">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Width="194">
                                    <materialDesign:PackIcon Kind="SettingsOutline" Margin="8,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Width="26" Height="26" Foreground="{DynamicResource ATextColor}"/>
                                    <TextBlock Margin="18,0,0,0" TextWrapping="NoWrap" Text="设置" FontSize="16" FontWeight="Normal" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{DynamicResource ATextColor}"/>
                                </StackPanel>
                            </MyC:znButton>
                        </Grid>
                        <Grid Height="46" Width="202" HorizontalAlignment="Left" Margin="0,2,0,0">
                            <Grid x:Name="AboutBtnBack">
                                <Rectangle Fill="{DynamicResource ALineColor}" RadiusY="6" RadiusX="6" Opacity="0.3"/>
                                <Rectangle Fill="{DynamicResource ButtonPAMP}" Width="4" Height="24" VerticalAlignment="Center" HorizontalAlignment="Left" RadiusY="2" RadiusX="2" Margin="2,0,0,0" />
                            </Grid>
                            <MyC:znButton x:Name="AboutBtn" ShowContent="True" VerticalAlignment="Stretch" BorderBrush="{x:Null}" Background="#00000000" HorizontalAlignment="Left" Width="202" Height="46" Click="MainBtn_Checked">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Width="194">
                                    <materialDesign:PackIcon Kind="AboutOutline" Margin="8,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Width="26" Height="26" Foreground="{DynamicResource ATextColor}"/>
                                    <TextBlock Margin="18,0,0,0" TextWrapping="NoWrap" Text="关于" FontSize="16" FontWeight="Normal" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{DynamicResource ATextColor}"/>
                                </StackPanel>
                            </MyC:znButton>
                        </Grid>
                    </StackPanel>
                </ListBox>
