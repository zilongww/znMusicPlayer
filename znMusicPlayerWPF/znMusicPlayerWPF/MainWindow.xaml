                                        <MyC:znButton x:Name="ShowBox_MusicDatas_ReduceButton" Content="-" Height="35" Width="35" HorizontalAlignment="Left" Background="{DynamicResource ButtonPAMP}" BorderBrush="{x:Null}" Foreground="{DynamicResource ATextColor_InColor}" Click="ShowBox_MusicDatas_ButtonsClick" FontSize="16"/>
