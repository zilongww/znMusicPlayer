                    <TextBlock x:Name="Title1" TextWrapping="NoWrap" Foreground="{DynamicResource ATextColor_InColor}" VerticalAlignment="Center" FontSize="16" Visibility="Visible" IsEnabled="False" Margin="10,13,196,13" Text="Title" FontWeight="Normal" Panel.ZIndex="5" TextTrimming="CharacterEllipsis"/>
                    <TextBlock x:Name="win11debugstext" TextWrapping="NoWrap" Foreground="{DynamicResource ATextColor_InColor}" VerticalAlignment="Top" FontSize="16" Visibility="Visible" IsEnabled="False" Margin="0,12,191,0" FontWeight="Normal" Panel.ZIndex="5" TextTrimming="CharacterEllipsis" FlowDirection="RightToLeft" HorizontalAlignment="Right"><Run Text=" "/></TextBlock>
                </Grid>
                <Grid x:Name="CenterPage" Margin="202,46,0,114" Width="{Binding ActualWidth, ElementName=TopBar}">
                    <Grid x:Name="TextAnimateGrid" VerticalAlignment="Top">
                        <TextBlock x:Name="PageTitle" HorizontalAlignment="Left" Margin="5,5,0,0" Text="主页" FontSize="28" Width="336" FontFamily="Microsoft YaHei" FontWeight="Light" Foreground="{DynamicResource ATextColor}"/>
                    </Grid>
                    <Grid x:Name="InPageAnimateGrid" Margin="0,46,0,0">
                        <ContentControl x:Name="InPage" Focusable="False"/>
                    </Grid>
                </Grid>
                <TextBox x:Name="debug" HorizontalAlignment="Left" Height="597" Margin="198,53,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="1031" Visibility="Collapsed"/>
                <Grid x:Name="PlayBarBack" Height="114" VerticalAlignment="Bottom" Margin="0,0,0,0" MouseLeftButtonDown="TopBar_MouseDown" Background="{DynamicResource ButtonPAMP}"/>
                <Rectangle Margin="203,53,804,456" Stroke="Black" Visibility="Collapsed">
                    <Rectangle.Fill>
                        <VisualBrush Visual="{Binding ElementName=InPage}" Stretch="UniformToFill"/>
                    </Rectangle.Fill>
                </Rectangle>
                <Grid x:Name="LeftBarMin" Height="46" Visibility="{Binding Visibility, ElementName=LeftBar}" Width="{Binding ActualWidth, ElementName=LeftBar}" VerticalAlignment="Top" Background="{DynamicResource ButtonPAMP}" HorizontalAlignment="Left">
                    <TextBlock x:Name="MenuText" TextWrapping="NoWrap" Foreground="{DynamicResource ATextColor_InColor}" VerticalAlignment="Center" FontSize="16" Visibility="Visible" IsEnabled="False" FontWeight="Normal" Margin="57,0,10,0" Text="菜单" Panel.ZIndex="5"/>
                    <MyC:znButton x:Name="Menu" CRadius="0" Focusable="False" ShowContent="True" HorizontalAlignment="Left" VerticalAlignment="Stretch" VerticalContentAlignment="Center" Width="52" Height="Auto" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{DynamicResource ATextColor_InColor}" FontSize="30" Click="Button_Click" FontFamily="Segoe UI Light">
                        <materialDesign:PackIcon Kind="Menu" Width="20" Height="26"/>
                    </MyC:znButton>
                </Grid>
            </Grid>
        </Grid>
        <ResizeGrip HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="{x:Null}" Background="{x:Null}" Panel.ZIndex="100" Visibility="Collapsed"/>

        <StackPanel x:Name="TopBtnGrid" VerticalAlignment="Top" HorizontalAlignment="Right" Background="{x:Null}" Orientation="Horizontal" Panel.ZIndex="5">
            <Grid>
                <MyC:znButton x:Name="FullScreenButton" CRadius="0" Focusable="False" Content="" Margin="0,0,0,0" VerticalAlignment="Center" Height="46" Background="#00000000" BorderBrush="{x:Null}" Foreground="{DynamicResource ATextColor_InColor}" FontSize="20" FontWeight="Normal" HorizontalAlignment="Right" Width="46" RenderTransformOrigin="0.5,0.5" Click="FullScreenButton_Click" AnimationColor="#64FFFFFF" EnterColor="#33FFFFFF"/>
                <Path x:Name="FullScreenPath" IsHitTestVisible="False" Data="{StaticResource 打开全屏}" Height="13" Stretch="UniformToFill" Width="13" VerticalAlignment="Center" Margin="0,0,0,0" HorizontalAlignment="Center" StrokeStartLineCap="Round" StrokeEndLineCap="Round" StrokeDashCap="Round" Fill="White" Stroke="White" StrokeThickness="0.45"/>
            </Grid>
            <MyC:znButton x:Name="WindowButton_Min" Focusable="False" Content="─" Height="46" CRadius="0" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{DynamicResource ATextColor_InColor}" FontSize="17" FontWeight="Normal" Click="WindowButton_Min_Click" Width="46" FontFamily="Microsoft YaHei UI" EnterColor="#33FFFFFF"/>
            <Grid>
                <MyC:znButton x:Name="WindowButton_Max" CRadius="0" Focusable="False" Content="" Background="#00000000" BorderBrush="{x:Null}" Foreground="{DynamicResource ATextColor_InColor}" FontSize="20" FontWeight="Normal" HorizontalAlignment="Right" Click="WindowButton_Max_Click" Height="46" Width="46" FontFamily="Microsoft YaHei UI" EnterColor="#33FFFFFF" MouseEnter="WindowButton_Max_MouseEnter" MouseLeave="WindowButton_Max_MouseLeave"/>
                <Path x:Name="WindowButton_Max_Path" IsHitTestVisible="False" Data="{StaticResource 最大化}" HorizontalAlignment="Center" Stretch="Fill" Fill="{DynamicResource ATextColor_InColor}" Width="12" Height="12" VerticalAlignment="Center" Stroke="{DynamicResource ATextColor_InColor}"/>
            </Grid>
            <MyC:znButton x:Name="WindowButton_Close" CRadius="0" Focusable="False" Content="✕" Background="#00000000" BorderBrush="{x:Null}" Foreground="{DynamicResource ATextColor_InColor}" FontSize="17" FontWeight="Normal" HorizontalAlignment="Right" Click="WindowButton_Close_Click" Height="46" Width="46" AnimationColor="#66FF0000" EnterColor="Red" EnterForegroundColor="White" FontFamily="Microsoft YaHei UI"/>
        </StackPanel>

        <ContentControl x:Name="MusicPage2" Content="ContentControl" Panel.ZIndex="3" Visibility="Collapsed"/>

        <Grid x:Name="ShowBoxUI" Visibility="Collapsed" Panel.ZIndex="100">
            <Rectangle x:Name="TheBack" Fill="#66000000"/>
            <Border Margin="10,106,10,115" CornerRadius="7" RenderTransformOrigin="0.5,0.5" Background="{DynamicResource BackColor}" VerticalAlignment="Center" HorizontalAlignment="Center" Visibility="Visible" MinHeight="120" MaxHeight="{Binding ActualHeight, ElementName=MainActivity}">
                <Border.RenderTransform>
                    <ScaleTransform x:Name="ShowBox_Scale" ScaleX="1" ScaleY="{Binding ScaleX, ElementName=ShowBox_Scale}"/>
                </Border.RenderTransform>
                <Border.Effect>
                    <DropShadowEffect BlurRadius="38" Direction="-90" ShadowDepth="6" Color="#FF3C3C3C" Opacity="0.5"/>
                </Border.Effect>
                <Grid x:Name="ShowBox_Grid">
                    <Grid x:Name="ShowBox_Normal" Visibility="Collapsed">
                        <TextBlock x:Name="ShowBox_Normal_Title" Margin="25,15,25,0" Text="标题" VerticalAlignment="Top" Foreground="{DynamicResource ATextColor}" FontSize="24" TextTrimming="CharacterEllipsis" TextAlignment="Left" FontWeight="Normal" HorizontalAlignment="Left"/>
                        <ListBox Margin="25,55,35,53" Style="{StaticResource AnimateListStyle}" ItemContainerStyle="{StaticResource ListBoxItemStyleSetting}" ScrollViewer.HorizontalScrollBarVisibility="Disabled" ScrollViewer.CanContentScroll="False" BorderBrush="{x:Null}" Background="{x:Null}" Foreground="{DynamicResource ALittleTextColor}">
