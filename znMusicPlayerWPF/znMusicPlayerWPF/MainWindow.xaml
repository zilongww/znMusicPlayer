        <Image x:Name="MainBackImage" RenderOptions.BitmapScalingMode="Fant" HorizontalAlignment="Center" Stretch="UniformToFill"/>
        <Grid x:Name="MainBackShadow" Background="White" Opacity="0"/>

        <Grid x:Name="MainUI">
