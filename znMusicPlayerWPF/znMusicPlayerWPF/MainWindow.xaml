                            </StackPanel>
                            <StackPanel x:Name="ShowBox_MusicDatas_AlbumIDText_s" Orientation="Horizontal">
                                <MyC:znButton x:Name="ShowBox_MusicDatas_AlbumIDText_Button" Content="专辑ID" HorizontalAlignment="Left" Margin="0" VerticalAlignment="Stretch" Background="{DynamicResource ButtonPAMP}" BorderBrush="{x:Null}" Foreground="{DynamicResource ATextColor_InColor}" Click="ShowBox_MusicDatas_ButtonsClick" Width="60" Height="23"/>
                                <TextBlock x:Name="ShowBox_MusicDatas_AlbumIDText" Text="专辑ID" Margin="5,0,0,0" Foreground="{DynamicResource ALittleTextColor}" FontSize="18" TextTrimming="CharacterEllipsis" TextWrapping="NoWrap" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Disabled" HorizontalAlignment="Left"/>
                            </StackPanel>
                            <StackPanel x:Name="ShowBox_MusicDatas_InMusicPage_s" Orientation="Vertical">
