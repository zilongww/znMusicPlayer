<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:znMusicPlayerWPF.Pages"
             xmlns:MyC="clr-namespace:znMusicPlayerWPF.MyC" x:Class="znMusicPlayerWPF.Pages.LoadPage"
             mc:Ignorable="d"
             d:DesignHeight="26" d:DesignWidth="26">
    <Grid>
        <MyC:SimpleLoading x:Name="loadingContent" Pause="True" StrokeThickness="1.4" Width="26" Foreground="{DynamicResource ButtonPAMP}" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" Height="26"/>
    </Grid>
</UserControl>
