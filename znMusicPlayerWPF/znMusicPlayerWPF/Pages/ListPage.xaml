<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:znMusicPlayerWPF.Pages"
             xmlns:b="http://schemas.microsoft.com/xaml/behaviors" x:Class="znMusicPlayerWPF.Pages.ListPage"
             mc:Ignorable="d" 
             d:DesignHeight="488" d:DesignWidth="805">
    <Grid>
        <ListBox x:Name="Listb" Style="{StaticResource AnimateListStyle}" ScrollViewer.CanContentScroll="False" ItemContainerStyle="{StaticResource ListBoxItemStyleSetting}" Background="{x:Null}" BorderBrush="{x:Null}">
            <StackPanel x:Name="Lists"/>
        </ListBox>
    </Grid>
</UserControl>
