<ScrollView
    x:Class="TewIMP.Controls.AutoScrollViewer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TewIMP.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" ExtentChanged="ScrollView_ExtentChanged"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" SizeChanged="sizeChanged" Unloaded="unloaded"
    HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Hidden"
    HorizontalScrollMode="Enabled" ContentOrientation="Both"
    ScrollCompleted="ScrollView_ScrollCompleted" ViewChanged="ScrollView_ViewChanged">
</ScrollView>
